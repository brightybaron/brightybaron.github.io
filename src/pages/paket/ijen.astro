---
import PaketLayout from "../../layouts/PaketLayout.astro";
const data = (await import("../../data/content.json")).default[1];
---

<PaketLayout
  title={data.title}
  bgImg={data.bgImg}
  durasi={data.durasi}
  highlight={data.highlight}
  descs={data.descs}
  destinasi={data.destinasi}
  fasilitas={data.fasilitas}
  harga={data.harga}
  itin={data.itin}
  images={data.images}
/>
